def equilibrium(a):

  if len(a) == 0:
    return -1

  left_sum = 0

  for i in range(len(a) - 1):

    right_sum = 0
    middle_point = i + 1
    left_sum += a[i]

    for j in range(middle_point + 1, len(a)):
      right_sum += a[j]

    if left_sum == right_sum:
      return middle_point
  
  return -1

print(input()) 
print(equilibrium([]))
